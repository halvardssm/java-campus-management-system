<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<BorderPane prefHeight="600.0" prefWidth="700.0" stylesheets="@../style.css" xmlns="http://javafx.com/javafx/10.0.2-internal" xmlns:fx="http://javafx.com/fxml/1" fx:controller="nl.tudelft.oopp.group39.controllers.Admin.AdminUListController">
   <top>
      <HBox spacing="10" styleClass="bar">
         <padding>
            <Insets bottom="15" left="15" right="15" top="15" />
         </padding>
         <children>
            <Text id="title" text="Admin: User List" />
         </children>
      </HBox>
   </top>
   <bottom>
      <HBox spacing="10" styleClass="bar">
         <padding>
            <Insets bottom="15" left="15" right="15" top="15" />
         </padding>
         <children>
            <Button id="backbtn" fx:id="backbtn" onAction="#switchBack" text="â—€ Back" />
         </children>
      </HBox>
   </bottom>
   <center>
      <AnchorPane prefHeight="200.0" prefWidth="200.0" BorderPane.alignment="CENTER">
         <children>
            <ImageView fitHeight="485.0" fitWidth="700.0" layoutY="-2.0" opacity="0.5" pickOnBounds="true" AnchorPane.bottomAnchor="-1.0" AnchorPane.leftAnchor="0.0" AnchorPane.rightAnchor="0.0" AnchorPane.topAnchor="0.0">
               <image>
                  <Image url="@../../../../../../../../TuDelft/download.png" />
               </image>
            </ImageView>
            <Text layoutX="220.0" layoutY="46.0" strokeType="OUTSIDE" strokeWidth="0.0" text="List of Users" underline="true">
               <font>
                  <Font name="System Bold" size="33.0" />
               </font>
            </Text>
            <Button fx:id="UAdd" layoutX="563.0" layoutY="11.0" mnemonicParsing="false" text="Add User">
               <font>
                  <Font name="System Bold" size="19.0" />
               </font>
            </Button>
            <TableView fx:id="usertable" editable="true" layoutY="55.0" opacity="0.75" prefHeight="432.0" prefWidth="700.0">
              <columns>
                 <TableColumn fx:id="idCol" prefWidth="200.0" text="ID" />
                 <TableColumn fx:id="emailCol" prefWidth="200.0" text="Name" />
                 <TableColumn fx:id="statusCol" prefWidth="150.0" text="Status" />
                 <TableColumn fx:id="userDelCol" prefWidth="75.0" text="Delete" />
                 <TableColumn fx:id="userUpCol" prefWidth="75.0" text="Update" />
              </columns>
            </TableView>
         </children>
      </AnchorPane>
   </center>
</BorderPane>
